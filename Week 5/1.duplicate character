#include<bits/stdc++.h>
using namespace std;
int main()
{
    cin>>test;
    for(int t=0;t<test;t++)
    int n;
    cin>>n;
    char a[n],b[26]={0};
    for(int i=0;i<n;i++)
    cin>>a[i];
    
     for(int i=0;i<n;i++)
     {
         b[int(a[i])-97]++;
        
     }
     int temp=0,max=b[0];
     if(n==1)
    cout<<1;
    else{
     for(int i=0;i<26;i++){
         
         if(b[i]>max)
           max=b[i];
     }
     if(max!=1)
     cout<<max;
     else
         cout<<"not present";
    }}
}
